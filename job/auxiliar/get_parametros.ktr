    <sql>select 
	(select
		vlr_parametro
	from
		aux_bi.apoio_parametros
	where
		cod_projeto = 'projeto_cortex'
		and cod_info_adicional = 'job_api_sidra_ibge'
		and cod_parametro = 'tabela') as TABELA,
	
	(select
		vlr_parametro
	from
		aux_bi.apoio_parametros
	where
		cod_projeto = 'projeto_cortex'
		and cod_info_adicional = 'job_api_sidra_ibge'
		and cod_parametro = 'periodo') as PERIODO,
	
	(select
		vlr_parametro
	from
		aux_bi.apoio_parametros
	where
		cod_projeto = 'projeto_cortex'
		and cod_info_adicional = 'job_api_sidra_ibge'
		and cod_parametro = 'variavel') as VARIAVEL ,
	
	(select
		vlr_parametro
	from
		aux_bi.apoio_parametros
	where
		cod_projeto = 'projeto_cortex'
		and cod_info_adicional = 'job_api_sidra_ibge'
		and cod_parametro = 'nivel_territorial') as NIVEL_TERRITORIAL,
	
	(select
		vlr_parametro
	from
		aux_bi.apoio_parametros
	where
		cod_projeto = 'projeto_cortex'
		and cod_info_adicional = 'job_api_sidra_ibge'
		and cod_parametro = 'url_api_ibge') as URL_API_IBGE</sql>
